# Use official Supra CLI image as base
FROM supralabs/supra-cli:latest

WORKDIR /app

# Install additional tools if needed
RUN apt-get update && apt-get install -y \
    git \
    curl \
    ca-certificates \
    && rm -rf /var/lib/apt/lists/*

# Copy Move project  
COPY veil-hub-v2/move/ /app/move/ || true
COPY supra/move_workspace/ /app/move/ || true

WORKDIR /app/move

# Default entry point
ENTRYPOINT ["/bin/bash"]
